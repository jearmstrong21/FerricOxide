extern void cPrint(i32 x, i32 y);

extern i8* malloc(i32 size);
extern void cPrintStr(i8* ptr);
extern void free(i8* ptr);

struct foo {
    i32 bar;
}

void run() {
    struct foo* x = new foo { bar: 6 };

    (*x).bar = 3;
    x->bar = 4 * x->bar;

    cPrint(2, x->bar);

    i8* str = malloc(6);
    *str = (i8) 72;
    str[1] = (i8) 101;
    str[2] = (i8) 108;
    str[3] = (i8) 108;
    str[4] = (i8) 111;
    str[5] = (i8) 0;
    cPrintStr(str);
    free(str);
}